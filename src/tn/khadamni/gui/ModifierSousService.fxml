<AnchorPane id="AnchorPane" fx:id="mybutton" prefHeight="476.0" prefWidth="795.0" stylesheets="@css/modifiersousservice.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.khadamni.gui.ModifierSousServiceController">
   <children>
