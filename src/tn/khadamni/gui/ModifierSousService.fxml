                  <ImageView fx:id="imageview" fitHeight="94.0" fitWidth="147.0" onMouseClicked="#insertimage" pickOnBounds="true" preserveRatio="true" />
               </children>
            </Pane>
            <TextField fx:id="txtNom" layoutX="136.0" layoutY="159.0" onKeyReleased="#verifnom" prefHeight="25.0" prefWidth="230.0" promptText="Nom sous service" />
